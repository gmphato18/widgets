{
    "name": "my-app",
    "version": "0.1.0",
    "private": true,
    "keywords": [
        "React",
        "widget",
        "UMD Library"
    ],
    "dependencies": {
        "@testing-library/jest-dom": "^5.16.5",
        "@testing-library/react": "^13.4.0",
        "@testing-library/user-event": "^13.5.0",
        "@types/jest": "^27.5.2",
        "@types/node": "^16.18.3",
        "@types/react": "^18.0.24",
        "@types/react-dom": "^18.0.8",
        "axios": "^1.1.3",
        "form-data": "^4.0.0",
        "node-fetch": "^3.2.10",
        "paths": "^0.1.1",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-scripts": "^5.0.1",
        "typescript": "^4.8.4"
    },
    "main": "src/index.tsx",
    "scripts": {
        "start": "react-scripts start",
        "start:local": "cross-env NODE_ENV=development webpack-dev-server --config webpack.config.js",
        "build:widget": "cross-env NODE_ENV=production parcel build src/index.tsx --no-source-maps -d dist -o calculator-widget",
        "build:table-demo": "SET NODE_ENV=production& webpack --config ./buildUtils/webpack.config.table.js --env=production",
        "build:calc-demo": "SET NODE_ENV=production& webpack --config ./buildUtils/webpack.config.calculator.js --env=production",
        "build:demo": "SET NODE_ENV=production& webpack --config ./buildUtils/webpack.config.table.js --env=production"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ],
        "parserOptions": {
            "babelOptions": {
                "presets": [
                    [
                        "babel-preset-react-app",
                        false
                    ]
                ]
            }
        }
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "@babel/core": "^7.19.6",
        "@babel/plugin-proposal-private-methods": "^7.18.6",
        "@babel/preset-react": "^7.18.6",
        "@parcel/compressor-brotli": "^2.7.0",
        "@parcel/compressor-gzip": "^2.7.0",
        "@parcel/config-default": "^2.7.0",
        "@parcel/reporter-bundle-buddy": "^2.7.0",
        "babel-polyfill": "^6.26.0",
        "babel-preset-react-app": "^10.0.1",
        "compression-webpack-plugin": "^10.0.0",
        "cross-env": "^7.0.3",
        "css-loader": "^6.7.1",
        "css-minimizer-webpack-plugin": "^4.2.2",
        "file-loader": "^6.2.0",
        "html-loader": "^4.2.0",
        "mini-css-extract-plugin": "^2.6.1",
        "parcel-bundler": "^1.12.5",
        "resolve-url-loader": "^5.0.0",
        "sass": "^1.55.0",
        "sass-loader": "^13.1.0",
        "source-map-loader": "^4.0.1",
        "svg-inline-loader": "^0.8.2",
        "webpack": "^5.74.0",
        "webpack-cli": "^4.10.0",
        "zlib": "^1.0.5"
    },
    "jsdelivr": "dist/my-app.min.js",
    "unpkg": "dist/my-app.min.js"
}
